              "application/json"
            ],
            "parameters": [
              {
                "name": "apikey",
                "in": "formData",
                "description": "apikey del usuario",
                "required": true,
                "type": "string"
              },
              {
                "name": "id",
                "in": "formData",
                "description": "id del usuario",
