            "application/json"
          ],
          "parameters": [
            {
              "name": "apikey",
              "in": "formData",
              "description": "apikey del usuario",
              "required": true,
              "type": "string"
            },
            {
              "name": "id",
              "in": "formData",
              "description": "id del usuario",
              "required": true,
              "type": "string"
            },
             {
              "name": "archivo",
              "in": "formData",
              "description": "video del usuario",
              "required": true,
              "type": "file"
            },

            {
              "name": "lat",
              "in": "formData",
              "description": "latitud del usuario",
              "required": true,
              "type": "string"
            },

            {
              "name": "long",
              "in": "formData",
              "description": "longitud del usuario",
              "required": true,
              "type": "string"
            }
            ],
          "responses": {
